redhat/turbolinux:
	/etc/rc.d/init.d/sqlrelay
	/etc/sysconfig/sqlrelay
	chkconfig
	/var/lock/subsys
mandrake:
slackware:
	entry in /etc/rc.d/rc.inet2
	/etc/rc.d/rc.sqlrelay
	/etc/sqlrelay
	/var/lock/subsys
debian:
	/etc/init.d/sqlrelay
	link to /etc/rcX.d
	/etc/sqlrelay
	/var/lock
netbsd:
	/etc/rc.d/sqlrelay
	entry in /etc/defaults/rc.conf
	/etc/sqlrelay
openbsd:
	entry in /etc/rc
	entry in /etc/rc.conf
	/etc/sqlrelay
freebsd:
	/usr/local/etc/rc.d
	/etc/sqlrelay
solaris:
openserver:
	/etc/init.d/sqlrelay
	/etc/sqlrelay

echo "***** SQLite *****************"

if ( test "$ENABLE_SQLITE" = "yes" )
then



if ( test "$cross_compiling" = "yes" )
then
		echo "cross compiling"
	if ( test -n "$GDBMPATH" )
	then
		GDBMINCLUDES="-I$GDBMPATH/include"
		GDBMLIBS="-L$GDBMPATH/lib -lgdbm"
		GDBMLIBSPATH="$GDBMPATH/lib"
	fi
else


SEARCHPATH=$LIBGDBMPATH
NAME=gdbm
HEADER=gdbm.h
LIBNAME=gdbm
LINKSTATIC=$STATICFLAG
LINKRPATH=$RPATHFLAG
USEFULLLIBPATH=
INCLUDESTRING=""
LIBSTRING=""
LIBPATH=""
STATIC=""
HEADERSANDLIBSPATH=""

eval "GDBMINCLUDES=\"\""
eval "GDBMLIBS=\"\""
eval "GDBMLIBSPATH=\"\""
eval "GDBMSTATIC=\"\""
if ( test -n "" )
then
	eval "=\"\""
fi


EXTRAPATH="$SEARCHPATH"
NAME="$NAME"
SUFFIX=""

SWPREFIXES="$EXTRAPATH / /usr"
if ( test -n "$NAME" )
then
	SWPREFIXES="$SWPREFIXES /usr/local/$NAME /opt/$NAME /usr/$NAME"
fi
SWPREFIXES="$SWPREFIXES /usr/local /usr/pkg"
if ( test -n "$NAME" )
then
	SWPREFIXES="$SWPREFIXES /usr/pkg/$NAME"
fi
SWPREFIXES="$SWPREFIXES /opt/sfw"
if ( test -n "$NAME" )
then
	SWPREFIXES="$SWPREFIXES /opt/sfw/$NAME"
fi
SWPREFIXES="$SWPREFIXES /usr/sfw"
if ( test -n "$NAME" )
then
	SWPREFIXES="$SWPREFIXES /usr/sfw/$NAME"
fi
SWPREFIXES="$SWPREFIXES /opt/csw /sw /usr/freeware /boot/common /resources/index"
SWPREFIXES="$SWPREFIXES /resources/firstworks"
if ( test -n "$NAME" )
then
	SWPREFIXES="$SWPREFIXES /Library/$NAME"
fi
SWPREFIXES="$SWPREFIXES /usr/local/firstworks"

if ( test -n "$SUFFIX" )
then
	SWPATHS=""
	for p in $SWPREFIXES
	do
		if ( test "$p" = "/" )
		then
			SWPATHS="$SWPATHS /$SUFFIX"
		else
			SWPATHS="$SWPATHS $p/$SUFFIX"
		fi
	done
	eval "SEARCHPATHS=\"$SWPATHS\""
else
	eval "SEARCHPATHS=\"$SWPREFIXES\""
fi

for path in $SEARCHPATHS
do
	if ( test -n "$path" -a -d "$path" )
	then

		TEMPLIBDIR=$LIBDIR
		if ( test "$path" = "/usr/freeware" )
		then
			TEMPLIBDIR=$LIBARCHDIR
		fi
		if ( test -z "$TEMPLIBDIR" )
		then
			TEMPLIBDIR=lib
		fi

		if ( test "$path" = "/" )
		then
			if ( test "$USEFULLLIBPATH" = "yes" )
			then

FOUNDHEADER=""
FOUNDLIB=""

if ( test -r "/$TEMPLIBDIR/lib$LIBNAME.$SOSUFFIX" )
then
	eval "FOUNDLIB=\"yes\""
fi

if ( test -n "$FOUNDLIB" )
then

if ( test -r "/usr/include/$HEADER" )
then
	eval "FOUNDHEADER=\"yes\""
fi

	if ( test -n "$FOUNDHEADER" )
	then
		eval ""
		eval "LIBPATH=\"/$TEMPLIBDIR\"; LIBSTRING=\"-Wl,/$TEMPLIBDIR/lib$LIBNAME.$SOSUFFIX\""
	fi
elif ( test -n "/$TEMPLIBDIR/lib$LIBNAME.a" )
then

if ( test -r "/$TEMPLIBDIR/lib$LIBNAME.a" )
then
	eval "FOUNDLIB=\"yes\""
fi

	if ( test -n "$FOUNDLIB" )
	then

if ( test -r "/usr/include/$HEADER" )
then
	eval "FOUNDHEADER=\"yes\""
fi

		if ( test -n "$FOUNDHEADER" )
		then
			eval ""
			eval "LIBSTRING=\"/$TEMPLIBDIR/lib$LIBNAME.a\"; STATIC=\"$LINKSTATIC\""
		fi
	fi
fi

			else

FOUNDHEADER=""
FOUNDLIB=""

if ( test -r "/$TEMPLIBDIR/lib$LIBNAME.$SOSUFFIX" )
then
	eval "FOUNDLIB=\"yes\""
fi

if ( test -n "$FOUNDLIB" )
then

if ( test -r "/usr/include/$HEADER" )
then
	eval "FOUNDHEADER=\"yes\""
fi

	if ( test -n "$FOUNDHEADER" )

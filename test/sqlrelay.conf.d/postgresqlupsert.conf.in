<?xml version="1.0"?>
<instances>

	<instance id="postgresqlupserttest" port="9000" socket="/tmp/test.socket" dbase="postgresql">
		<users>
			<user user="test" password="test"/>
		</users>
		<translations>
			<translation module="normalize"/>
		</translations>
		<triggers>
			<trigger module="upsert" when="after">
				<errors>
					<error string="duplicate key value"/>
				</errors>
				<tables>
					<table name="student">
						<primarykey name="id"/>
						<column name="firstname"/>
						<column name="lastname"/>
					</table>
				</tables>
			</trigger>
		</triggers>
		<connections>
			<connection string="host=postgresql;user=testuser;password=testpassword;db=win7x64;lastinsertidfunction=testfunc();typemangling=lookup;tablemangling=lookup"/>
		</connections>
	</instance>

</instances>

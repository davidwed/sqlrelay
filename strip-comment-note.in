#!/bin/sh

# if the file doesn't exist or is unreadable, exit gracefully
if ( test ! -r "$1" )
then
	exit 0
fi

STRIP=@STRIP@

if ( test "$STRIP" != "touch" )
then
	if ( test -n "`grep -l 'Generated by ltmain.sh' $1`" )
	then
		. ./$1
		if ( test -n "$pic_object" )
		then
			if ( test -r "$pic_object" )
			then
        			$STRIP -x -R .note -R .comment -R .note.ABI-tag $pic_object
			fi
			if ( test -r "`basename $1`/$pic_object" )
			then
        			$STRIP -x -R .note -R .comment -R .note.ABI-tag `basename $1`/$pic_object
			fi
		fi
		if ( test -n "$non_pic_object" )
		then
			if ( test -r "$non_pic_object" )
			then
        			$STRIP -x -R .note -R .comment -R .note.ABI-tag $non_pic_object
			fi
			if ( test -r "`basename $1`/$non_pic_object" )
			then
        			$STRIP -x -R .note -R .comment -R .note.ABI-tag `basename $1`/$non_pic_object
			fi
		fi
	else
        	$STRIP -x -R .note -R .comment -R .note.ABI-tag $1
	fi
fi

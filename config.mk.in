SHELL = /bin/sh
VPATH = @srcdir@

#abs_srcdir = @abs_srcdir@
#top_srcdir = $(abs_srcdir)
#abs_builddir = @abs_builddir@
#top_builddir = $(abs_builddir)

SQLR_VERSION = @SQLR_VERSION@
SONAME_VERSION_INFO = @SONAME_VERSION_INFO@

DESTDIR =

# installation directories
prefix = @prefix@
exec_prefix= @exec_prefix@
includedir = @includedir@
libdir = @libdir@
javadir = ${exec_prefix}/java
libexecdir = @libexecdir@
bindir = @bindir@
localstatedir = @localstatedir@
sysconfdir = @sysconfdir@
mandir = @mandir@
datadir = @datadir@
docdir = ${datadir}/doc/sqlrelay
tmpdir = ${localstatedir}/sqlrelay/tmp
cachedir = ${localstatedir}/sqlrelay/cache
debugdir = ${localstatedir}/sqlrelay/debug

# compiler to use
CC = @CC@
CXX = @CXX@

# includes and defines (-I and -D flags)
BASECPPFLAGS = @CPPFLAGS@ @PTHREADINCLUDES@
TMPDIRCPPFLAG = -DTMP_DIR=\"$(tmpdir)\"
DEBUGDIRCPPFLAG = -DDEBUG_DIR=\"$(debugdir)\"
CONFIGFILECPPFLAG = -DDEFAULT_CONFIG_FILE=\"$(sysconfdir)/sqlrelay.conf\"

# link flags (-s, -static, -melf, -belf, etc.)
LDFLAGS = @LDFLAGS@
STATICLINK = @STATICLINK@

# debug and optimization flags (-g, -O2, etc.)
CXXFLAGS = @CXXFLAGS@ @PIPE@ @WNOLONGDOUBLE@
CFLAGS = @CFLAGS@ @PIPE@ @WNOLONGDOUBLE@

# libtool command
LIBTOOL = @LIBTOOL@
LIBTOOLGCC = @LIBTOOLGCC@

# install command
LTINSTALL = $(LIBTOOL) --mode=install $(top_builddir)/install-sh -c
INSTALL = $(top_builddir)/install-sh -c

# mkinstalldirs command
MKINSTALLDIRS = $(top_builddir)/mkinstalldirs

# commands to erase files and directories
RM = rm -f
RMTREE = rm -rf

# command to create a symbolic link
SYMLINK = ln -s

# ps command
PSDEFINE = -DPS=\"@PS@\"

# uname -s
UNAME = @UNAME@

# rpm build directory
RPM_BUILD_DIR = @RPM_BUILD_DIR@

# extra libs
EXTRALIBS = @LIBS@

# windows environment
MINGW32 = @MINGW32@

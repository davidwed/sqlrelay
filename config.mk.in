SHELL = /bin/sh
VPATH = @srcdir@

abs_srcdir = @abs_srcdir@
top_srcdir = $(abs_srcdir)
abs_builddir = @abs_builddir@
top_builddir = $(abs_builddir)

SQLR_VERSION = @SQLR_VERSION@
SONAME_VERSION_INFO = @SONAME_VERSION_INFO@

# installation directories
prefix = @prefix@
exec_prefix= @exec_prefix@
incdir = @includedir@
libdir = @libdir@
javadir = ${exec_prefix}/java
libexecdir = @libexecdir@
bindir = @bindir@
localstatedir = @localstatedir@
sysconfdir = @sysconfdir@
localstatedir = @localstatedir@
mandir = @mandir@
docdir = ${prefix}/doc/sqlrelay
tmpdir = ${localstatedir}/sqlrelay/tmp
cachedir = ${localstatedir}/sqlrelay/cache
debugdir = ${localstatedir}/sqlrelay/debug

# compiler to use
CC = @CC@
CXX = @CXX@

# includes and defines (-I and -D flags)
BASECPPFLAGS = @CPPFLAGS@
TMPDIRCPPFLAG = -DTMP_DIR=\"$(tmpdir)\"
DEBUGDIRCPPFLAG = -DDEBUG_DIR=\"$(debugdir)\"
CONFIGFILECPPFLAG = -DDEFAULT_CONFIG_FILE=\"$(sysconfdir)/sqlrelay.conf\"

# link flags (-s, -static, -melf, -belf, etc.)
LDFLAGS = @LDFLAGS@
STATICLINK = @STATICLINK@
INSTALLSTRIP = @INSTALLSTRIP@

# debug and optimization flags (-g, -O2, etc.)
CXXFLAGS = @CXXFLAGS@
CFLAGS = @CFLAGS@

# libtool command
LIBTOOL = @LIBTOOL@

# commands to erase files and directories
RM = rm -f
RMTREE = rm -rf

# command to create a symbolic link
SYMLINK = ln -s

# ps command
PSDEFINE = -DPS=\"@PS@\"


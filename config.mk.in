SHELL = /bin/sh
VPATH = @srcdir@

SQLR_VERSION = @SQLR_VERSION@
SONAME_VERSION_INFO = @SONAME_VERSION_INFO@

# installation directories
prefix = @prefix@
exec_prefix= @exec_prefix@
incdir = @includedir@
libdir = @libdir@
javadir = ${exec_prefix}/java
libexecdir = @libexecdir@
bindir = @bindir@
localstatedir = @localstatedir@
sysconfdir = @sysconfdir@
localstatedir = @localstatedir@
mandir = @mandir@
docdir = ${prefix}/doc/sqlrelay
tmpdir = ${localstatedir}/sqlrelay/tmp
cachedir = ${localstatedir}/sqlrelay/cache
debugdir = ${localstatedir}/sqlrelay/debug

# compiler to use
CC = @CC@
CXX = @CXX@

# includes and defines (-I and -D flags)
BASECPPFLAGS = @CPPFLAGS@ -DTMP_DIR=\"$(tmpdir)\" -DDEBUG_DIR=\"$(debugdir)\" -DDEFAULT_CONFIG_FILE=\"$(sysconfdir)/sqlrelay.conf\"

# link flags (-s, -static, -melf, -belf, etc.)
LDFLAGS = @LDFLAGS@
STATICLINK = @STATICLINK@

# debug and optimization flags (-g, -O2, etc.)
CXXFLAGS = -pedantic -w @CXXFLAGS@
CFLAGS = -pedantic -w @CFLAGS@
DEBUGCXXFLAGS = -pedantic -w @DEBUGCXXFLAGS@
DEBUGCFLAGS = -pedantic -w @DEBUGCFLAGS@

# libtool command
LIBTOOL = @LIBTOOL@

# command to build a shared object library
DYNAMICAR = @DYNAMICAR@

# command to build a static library
STATICAR = ar cr

# commands to erase files and directories
RM = rm -f
RMTREE = rm -rf

# command to create a symbolic link
SYMLINK = ln -s

# command to create a blank file
TOUCH = touch

# ps command
PSDEFINE = -DPS=\"@PS@\"

# uname -s
UNAME = @UNAME@

# rpm build directory
RPM_BUILD_DIR = @RPM_BUILD_DIR@

# extra libs
EXTRALIBS = @LIBS@

# windows environment
MINGW32 = @MINGW32@
CYGWIN = @CYGWIN@
UWIN = @UWIN@

# rudiments library
RUDIMENTSPATH = @RUDIMENTSPATH@
RUDIMENTSLIBS = @RUDIMENTSLIBS@ 
RUDIMENTSINCLUDES = @RUDIMENTSINCLUDES@ 

# perl
HAVE_PERL = @HAVE_PERL@
PERL = @PERL@



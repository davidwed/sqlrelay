HAVE_PYTHON = @HAVE_PYTHON@
PYTHONINCLUDES = @PYTHONINCLUDES@
PYTHONLIBS = @PYTHONLIBS@
PYTHONOBJ = @PYTHONOBJ@
PYTHONDIR = @PYTHONDIR@



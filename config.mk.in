OUT = -o 
BASECPPFLAGS = @CPPFLAGS@ @PTHREADINCLUDES@
EXTRACPPFLAGS =
CXXFLAGS = @CXXFLAGS@ @WALL@ @PIPE@ @WNOLONGDOUBLE@ @WNOOVERLOADEDVIRTUAL@

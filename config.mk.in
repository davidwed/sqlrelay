# command separator
AND = ;


# slash
SLASH = /


# shell
SHELL = @SHELL@


# libtool command
LIBTOOL = @LIBTOOL@


# compile commands
LTCOMPILE = $(LIBTOOL) --mode=compile
CC = @CC@
CXX = @CXX@
